{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  },
  "hosting": [{
    "target": "default",
    "public": "website",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/addImmuneHero",
        "function": "addImmuneHero"
      },
      {
        "source": "/addStakeHolder",
        "function": "addStakeHolder"
      },
      {
        "source": "/getStakeHoldersInZipCodeRangeAsJson",
        "function": "getStakeHoldersInZipCodeRangeAsJson"
      },
      {
        "source": "/getAllStakeHoldersAsJson",
        "function": "getAllStakeHoldersAsJson"
      },
      {
        "source": "/getAllImmuneHeroesNutsAsJson",
        "function": "getAllImmuneHeroesNutsAsJson"
      },
      {
        "source": "/doneVerifyStakeholderPin",
        "function": "doneVerifyStakeholderPin"
      },
      {
        "source": "/contactStakeholder",
        "function": "contactStakeholder"
      },
      {
        "source": "/demo/pins",
        "function": "pin_locations"
      },
      {
        "source": "/demo/regions",
        "function": "regions"
      },
      {
        "source": "/demo/details_html",
        "function": "details_html"
      },
      {
        "source": "/map/pins",
        "function": "pin_locations"
      },
      {
        "source": "/map/regions",
        "function": "regions"
      },
      {
        "source": "/map/details_html",
        "function": "details_html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  {
    "target": "dev",
    "public": "new-website/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/addImmuneHero",
        "function": "addImmuneHero"
      },
      {
        "source": "/addStakeHolder",
        "function": "addStakeHolder"
      },
      {
        "source": "/getStakeHoldersInZipCodeRangeAsJson",
        "function": "getStakeHoldersInZipCodeRangeAsJson"
      },
      {
        "source": "/getAllStakeHoldersAsJson",
        "function": "getAllStakeHoldersAsJson"
      },
      {
        "source": "/getAllImmuneHeroesNutsAsJson",
        "function": "getAllImmuneHeroesNutsAsJson"
      },
      {
        "source": "/doneVerifyStakeholderPin",
        "function": "doneVerifyStakeholderPin"
      },
      {
        "source": "/contactStakeholder",
        "function": "contactStakeholder"
      },
      {
        "source": "/demo/pins",
        "function": "pin_locations"
      },
      {
        "source": "/demo/regions",
        "function": "regions"
      },
      {
        "source": "/demo/details_html",
        "function": "details_html"
      },
      {
        "source": "/map/pins",
        "function": "pin_locations"
      },
      {
        "source": "/map/regions",
        "function": "regions"
      },
      {
        "source": "/map/details_html",
        "function": "details_html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }]
}
